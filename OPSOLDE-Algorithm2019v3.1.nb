Cell["\<\
This function runs the first case of the theorem with given m and n. It \
produces coefficients of the solution in terms of c[0] and coefficients of \
the initial differential equation, as well as sufficient conditions and \
necessary condition. The function returns array of m + 1 coefficients \
represented as determinant of matrix over determinant of another matrix, \
array of n - 2 sufficient conditions represented as determinants of matrixes, \
and equation representing necessary condition.\
\>", "Text",
 CellChangeTimes->{
  3.6724153650089626`*^9, {3.672415419326213*^9, 3.6724154198865957`*^9}, {
   3.6724154798046083`*^9, 3.6724155354244137`*^9}, {3.6724155888076854`*^9, 
   3.6724156252772064`*^9}, {3.6724156600027685`*^9, 3.672415766999484*^9}, {
   3.6724162607205763`*^9, 3.6724162742048635`*^9}, {3.67241639087852*^9, 
   3.6724164334511504`*^9}, {3.6724164965729127`*^9, 3.672416546224444*^9}, {
   3.672477487618597*^9, 3.6724776199073753`*^9}, {3.6727508683238273`*^9, 
   3.6727508747883987`*^9}, 3.67483486268112*^9, {3.6748356148271003`*^9, 
   3.674835626600436*^9}, {3.6751964382717357`*^9, 3.6751964545143185`*^9}, {
   3.6755266827134333`*^9, 3.6755267203219247`*^9}, 3.6755269928315835`*^9, {
   3.675527074166306*^9, 3.675527074170297*^9}, 3.6755316382354393`*^9, {
   3.6756097822045264`*^9, 3.675609819698532*^9}, 3.6757865041667585`*^9, 
   3.676117847905527*^9, {3.6761335063797894`*^9, 3.6761335405113225`*^9}, {
   3.676200512479615*^9, 3.6762005251688185`*^9}, {3.6762005867355824`*^9, 
   3.6762005867375836`*^9}, {3.676200647372735*^9, 3.676200675891579*^9}, {
   3.6763074034702487`*^9, 3.676307431350424*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Theorem1Fn", "[", 
   RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "cond", ",", "coef", ",", "suf", ",", "nec", ",", "cc", ",", "c0col", 
      ",", "crest", ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"cond", "=", 
      RowBox[{"GeneralRecursionFn", "[", 
       RowBox[{"m", ",", "n", ",", "0", ",", "0"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"coef", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"cond", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "m"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"nec", "=", 
      RowBox[{
       RowBox[{"cond", "[", 
        RowBox[{"[", 
         RowBox[{
          RowBox[{"m", "+", "n", "-", "1"}], ",", 
          RowBox[{"m", "+", "1"}]}], "]"}], "]"}], "\[Equal]", "0"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"suf", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Append", "[", 
         RowBox[{"coef", ",", 
          RowBox[{"cond", "[", 
           RowBox[{"[", 
            RowBox[{"i", "+", "m"}], "]"}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"n", "-", "2"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cc", "=", 
      RowBox[{"{", 
       RowBox[{"c", "[", "0", "]"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"c0col", "=", 
      RowBox[{"-", 
       RowBox[{"coef", "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "1"}], "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"crest", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"coef", "[", 
         RowBox[{"[", 
          RowBox[{"i", ",", 
           RowBox[{"j", "+", "1"}]}], "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "m"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "m"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "m"}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", "copy", "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"copy", "=", "crest"}], ";", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"copy", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "i"}], "]"}], "]"}], "=", "c0col"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"cc", "=", 
           RowBox[{"Append", "[", 
            RowBox[{"cc", ",", 
             RowBox[{"{", 
              RowBox[{"copy", ",", "crest"}], "}"}]}], "]"}]}], ";"}]}], 
        "\[IndentingNewLine]", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{"cc", ",", "suf", ",", "nec", ",", "cond"}], "}"}], "]"}], 
     ";"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
