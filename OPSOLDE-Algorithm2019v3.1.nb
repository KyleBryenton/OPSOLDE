                 RowBox[{"Continue", "[", "]"}]}], "]"}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"cond", "[", 
                 RowBox[{"[", 
                  RowBox[{"row", ",", "col"}], "]"}], "]"}], "=", 
                RowBox[{"FullSimplify", "[", 
                 RowBox[{"coefFormula", "[", 
                  RowBox[{"n", ",", "s", ",", "k", ",", "j"}], "]"}], 
                 "]"}]}]}]}], "\[IndentingNewLine]", "]"}]}], "]"}], ";"}]}], 
        "\[IndentingNewLine]", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "cond", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
